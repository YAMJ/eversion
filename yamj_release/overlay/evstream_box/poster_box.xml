<?xml version="1.0" encoding="UTF-8"?>
<overlay>
    <layer after="true">
        <name>videosource</name>
		<left>0</left>
		<top>0</top>
		<align>left</align>
		<valign>top</valign>
        <images>
            <image value="SDTV" filename="/covers/poster_sdtv.png"/>
            <image value="HDTV" filename="/covers/poster_hdtv.png"/>
            <image value="EDTV" filename="/covers/poster_edtv.png"/>
            <image value="HDDVD" filename="/covers/poster_hddvd.png"/>
